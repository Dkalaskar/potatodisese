{
    "functions": {
      "api/main.py": {
        "runtime": "python3.10",
        "memory": 1024
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/main.py"
      },
      {
        "source": "/(.*)",
        "destination": "/api/main.py"
      }
    ]
  }